<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.firesystemcs.dao.VideoDao">
    <insert id="addAvid" parameterType="com.example.firesystemcs.entity.Video">
        INSERT INTO video(eid, time, videoname)
        VALUES (#{eid}, #{time}, #{videoname})
    </insert>
</mapper>